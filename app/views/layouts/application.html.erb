<!DOCTYPE html>
<html>
  <head>
    <title>Borjan</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="fb:app_id" content="1344945245522711" />
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet"/>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <%= favicon_link_tag 'favicon.ico' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
  <div id="fb-root"></div>
    <script>(function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.6&appId=1344945245522711";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>

    <div id="scrolltop" class="hidden-xs "><i class="fa fa-angle-up"></i></div>
    <div id="page-wrapper">
    <%#= flash[:notice] %>
      <%= render "shared/messages" %>
      <%= render "shared/header" %>
      <%= render "popup_partials/login_popup" %>
      <%= render "popup_partials/forgot_password" %>

      <%= yield %>

      <%= render "shared/footer" %>
    </div>

  </body>
</html>
